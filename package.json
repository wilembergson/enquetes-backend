    "test:unit": "jest test -- --watch -c jest-unit-config.js",
    "test:integration": "jest test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
